{
    "name": "<repo_name_here>",
    "license": "MIT",
    "devDependencies": {
        "@semantic-release/github": "^8.0.7",
        "git-cz": "^4.9.0",
        "semantic-release": "^19.0.5"
    },
    "private": true,
    "release": {
        "plugins": [
            "@semantic-release/commit-analyzer",
            "@semantic-release/release-notes-generator",
